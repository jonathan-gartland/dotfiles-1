VERBOSE=off
MAILDIR=$HOME/Maildir
DEFAULT=$MAILDIR/.INBOX/
PMDIR=$HOME/.procmail
LOGFILE=$PMDIR/log/procmail

INCLUDERC=$PMDIR/spam.rc
INCLUDERC=$PMDIR/mailing_lists.rc
INCLUDERC=$PMDIR/github.rc
INCLUDERC=$PMDIR/jobs.rc
INCLUDERC=$PMDIR/hemlock_forest.rc
INCLUDERC=$PMDIR/knightcx.rc
INCLUDERC=$PMDIR/misc.rc

# Archive remaining messages
:0c
*
.Archives/ # to Archives

# last
:0:
$DEFAULT

